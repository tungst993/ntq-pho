{
  "name": "react-native-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cd android &&./gradlew clean && cd .. && npx react-native run-android ",
    "ios": "cd ios && xcodebuild clean && cd .. && npx react-native run-ios",
    "android-release": "npx react-native run-android --variant=release",
    "start": "react-native start",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "lint": "yarn tsc & eslint --ext .ts,.tsx .",
    "apk": "cd android &&./gradlew clean && ./gradlew assembleRelease && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "schema:codegen": "gqlg --schemaFilePath ./schema/schema.graphql --destDirPath ./__generated__ --depthLimit 5 && prettier --write --loglevel silent __generated__/**/*.gql",
    "codegen": "graphql-codegen --config codegen/codegen.js && yarn schema:codegen",
    "typegen": "graphql-codegen --config codegen/typegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.3.8",
    "@gorhom/bottom-sheet": "3.0.1",
    "@hookform/resolvers": "1.2.0",
    "@react-native-community/async-storage": "2.0.0-rc.1",
    "@react-native-community/async-storage-backend-legacy": "2.0.0-rc.0",
    "@react-native-community/cameraroll": "^4.0.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-google-signin/google-signin": "^6.0.0",
    "@react-native-picker/picker": "^1.9.11",
    "@react-navigation/bottom-tabs": "5.11.2",
    "@react-navigation/native": "5.8.10",
    "@react-navigation/stack": "5.12.8",
    "@types/react-native-snap-carousel": "^3.8.2",
    "@types/react-native-video": "^5.0.4",
    "@welldone-software/why-did-you-render": "6.0.4",
    "add": "^2.0.6",
    "axios": "0.21.1",
    "extract-files": "9.0.0",
    "flat": "5.0.2",
    "graphql": "15.4.0",
    "i18next": "19.8.4",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-hook-form": "6.13.1",
    "react-i18next": "11.8.5",
    "react-native": "0.63.3",
    "react-native-bootsplash": "3.1.2",
    "react-native-complete-mentions": "^1.0.9",
    "react-native-elements": "3.0.1",
    "react-native-fast-image": "8.3.4",
    "react-native-flash-message": "^0.1.23",
    "react-native-gesture-handler": "1.9.0",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-hooks": "0.9.0",
    "react-native-image-crop-picker": "^0.36.0",
    "react-native-image-filter-kit": "^0.7.3",
    "react-native-indicators": "^0.17.0",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "2.0.1",
    "react-native-modal": "^11.7.0",
    "react-native-modalize": "^2.0.8",
    "react-native-modest-checkbox": "^3.3.0",
    "react-native-nodemediaclient": "^0.2.15",
    "react-native-picker-select": "^8.0.4",
    "react-native-pose": "^0.9.1",
    "react-native-reanimated": "1.13.2",
    "react-native-responsive-dimensions": "^3.1.1",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "2.16.1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-super-grid": "^4.1.1",
    "react-native-svg": "^12.1.0",
    "react-native-toast-message": "^1.4.9",
    "react-native-vector-icons": "^8.1.0",
    "react-native-video": "^5.1.1",
    "react-native-zalo-kit": "^1.0.9",
    "recoil": "^0.1.3",
    "rn-material-ui-textfield": "^1.0.2",
    "rn-placeholder": "^3.0.3",
    "subscriptions-transport-ws": "^0.9.18",
    "yarn": "^1.22.10",
    "yup": "0.32.8"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/runtime": "7.12.5",
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/near-operation-file-preset": "1.17.13",
    "@graphql-codegen/schema-ast": "1.18.1",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/extract-files": "8.1.0",
    "@types/flat": "5.0.1",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.16",
    "@types/react": "16.9.56",
    "@types/react-native": "0.63.42",
    "@types/react-test-renderer": "16.9.3",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-prettier": "3.3.0",
    "gql-generator": "1.0.12",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "metro-react-native-babel-preset": "0.64.0",
    "prettier": "2.2.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.13.1",
    "typescript": "4.1.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
